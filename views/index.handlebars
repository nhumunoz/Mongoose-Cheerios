<div>
  <h1 class="card z-depth-3 center-align">Read the Latest from "The Climbing Magazine"</h1>
  <div class="row">
    <h5 class="card z-depth-3 center-align col 10">Click 'Scrape' for articles</h5>
  </div>
</div>

<div class="row">
  <div class="col s2"></div>
  <div class="col s8 left-align">
    {{#each Article}}
    <div class="card-panel">
      <h4>{{this.title}}</h4>
      <a href="/readArticle/{{this._id}}" class="waves-effect waves-light btn" id="btnSignUp">Read Article</a>
    </div>
    {{/each}}
  </div>
</div>